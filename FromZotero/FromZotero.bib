@article{dickieElectronMicroscopicInvestigations1969,
  title = {Electron Microscopic Investigations on the Nature of Petroleum Asphaltics},
  author = {Dickie, John P and Haller, Martin N and Yen, Teh Fu},
  date = {1969-03-01},
  journaltitle = {Journal of Colloid and Interface Science},
  shortjournal = {Journal of Colloid and Interface Science},
  volume = {29},
