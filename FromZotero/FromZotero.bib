  pages = {475--484},
  issn = {0021-9797},
  doi = {10.1016/0021-9797(69)90131-3},
  url = {https://www.sciencedirect.com/science/article/pii/0021979769901313},
  urldate = {2024-05-26},
  abstract = {An examination of the electron micrographs of four native asphalties, a vanadium-enriched asphaltene, an asphaltene from a visbreaker tar, a resin, and a gilsonite, has revealed in all cases irregular polyhedral shaped particles of average diameter 20–30A. These sizes are similar to the diameters of the aromatic portion of the unit asphaltene cell as estimated from high-angle X-ray studies, suggesting that the associated aliphatic and naphthenic portions of the molecule must be closely bound to the aromatic nucleus. Evidence for particle association into larger clusters was obtained for the vanadium-enriched asphaltene and from the visbreaker tar, and was also noted in samples prepared from concentrated pyridine solutions. The larger particles (150–300A) observed for the vanadium-enriched asphaltene (Boscan), and to some extent for the Boscan asphaltene itself, are thought to be due to the high metal content. Since the smallest particles observed for the native asphaltene are larger than the minimum resolution of the electron microscope, it seems possible that these particles represent the ultimate unit cells of the asphaltenes.},
  file = {files/158/0021979769901313.html}
}

@online{dimandCowlesCommissionFoundation2019,
  type = {SSRN Scholarly Paper},
  title = {The {{Cowles Commission}} and {{Foundation}} for {{Research}} in {{Economics}}},
  author = {Dimand, Robert W.},
  date = {2019-11-27},
  number = {3495952},
  location = {Rochester, NY},
  doi = {10.2139/ssrn.3495952},
  url = {https://papers.ssrn.com/abstract=3495952},
  urldate = {2024-09-08},
  abstract = {Founded in 1932 by a newspaper heir disillusioned by the failure of forecasters to predict the Great Crash, the Cowles Commission promoted the use of formal mathematical and statistical methods in economics, initially through summer research conferences in Colorado and through support of the Econometric Society (of which Alfred Cowles was secretary-treasurer for decades). After moving to the University of Chicago in 1939, the Cowles Commission sponsored works, many later honored with Nobel Prizes but at the time out of the mainstream of economics, by Haavelmo, Hurwicz and Koopmans on econometrics, Arrow and Debreu on general equilibrium, Yntema and Mosak on general equilibrium in international trade theory, Arrow on social choice, Koopmans on activity analysis, Klein on macroeconometric modelling, Lange, Marschak and Patinkin on macroeconomic theory, and Markowitz on portfolio choice, but came into intense methodological, ideological and personal conflict with the emerging “Chicago school.” This conflict led the Cowles Commission to move to Yale in 1955 as the Cowles Foundation, directed by James Tobin (who had declined to move to Chicago to direct it). The Cowles Foundation remained a leader in the more technical areas of economics, notably with Tobin’s “Yale school” of monetary theory, Scarf’s computable general equilibrium, Shubik in game theory, and later Phillips and Andrews in econometric theory but as formal methods in economic theory and econometrics pervaded the discipline of economics, Cowles (like the Econometric Society) became less distinct from the rest of economics. This entry is part of an archivally-based history of the Cowles Commission and Foundation commissioned by the Cowles Foundation.},
  langid = {english},
  pubstate = {prepublished},
  keywords = {Cowles approach to econometrics,Cowles Commission,Formalism in economics,Mathematics in economics},
  file = {files/622/Dimand - 2019 - The Cowles Commission and Foundation for Research .pdf}
}

