@article{hudsonRealityJeanPerrins2020,
  title = {The {{Reality}} of {{Jean Perrin}}'s {{Atoms}} and {{Molecules}}},
  author = {Hudson, Robert},
  date = {2020-03},
  journaltitle = {The British Journal for the Philosophy of Science},
  volume = {71},
  number = {1},
  pages = {33--58},
  publisher = {The University of Chicago Press},
  issn = {0007-0882},
  doi = {10.1093/bjps/axx054},
  url = {https://www.journals.uchicago.edu/doi/abs/10.1093/bjps/axx054},
  urldate = {2024-05-24},
  abstract = {Jean Perrin’s proof in the early-twentieth century of the reality of atoms and molecules is often taken as an exemplary form of robustness reasoning, where an empirical result receives validation if it is generated using multiple experimental approaches. In this article, I describe in detail Perrin’s style of reasoning, and locate both qualitative and quantitative forms of argumentation. Particularly, I argue that his quantitative style of reasoning has mistakenly been viewed as a form of robustness reasoning, whereas I believe it is something different, what I call ‘calibration’. From this perspective, I re-evaluate recent interpretations of Perrin provided by Stathis Psillos, Peter Achinstein, Alan Chalmers, and Bas van Fraassen, all of whom read Perrin as a robustness reasoner, though not necessarily in the same sort of way. I then argue that by viewing Perrin as a ‘calibration’ reasoner we gain a better understanding of why he believes himself to have established the reality of atoms and molecules. To conclude, I provide an alternative and more productive understanding of the basis of the dispute between realists and anti-realists. 1{$\quad$}Introduction 2{$\quad$}Perrin’s Reasoning: The Qualitative Argument 3{$\quad$}Perrin’s Reasoning: The Quantitative Argument 4{$\quad$}Perrin’s Realism 5{$\quad$}Psillos, Achinstein, Chalmers, and van Fraassen on Understanding Perrin 6{$\quad$}Conclusion},
  file = {files/221/Hudson - 2020 - What Was Perrin Really Doing in His Proof of the R.pdf}
}

@article{hudsonWhatWasPerrin2020,
  title = {What {{Was Perrin Really Doing}} in {{His Proof}} of the {{Reality}} of {{Atoms}}?},
  author = {Hudson, Robert},
  date = {2020-03},
  journaltitle = {HOPOS: The Journal of the International Society for the History of Philosophy of Science},
  volume = {10},
  number = {1},
  pages = {194--218},
  publisher = {The University of Chicago Press},
  issn = {2152-5188},
  doi = {10.1086/707752},
  url = {https://www.journals.uchicago.edu/doi/abs/10.1086/707752},
  urldate = {2024-05-24},
  abstract = {It is commonly thought that Jean Perrin argued for the reality of atoms in the early twentieth century by using what Wesley Salmon calls a “common cause” argument, also known as robustness reasoning. After citing some concerns with this interpretation of Perrin, I offer a different interpretation of Perrin’s work that more closely depicts the details of Perrin’s reasoning in his relevant published writings. I then offer a historical argument that supports this interpretation and discuss the philosophical merits of Perrin’s style of reasoning as I have presented it. I close by showing how my interpretation is distinct from previous interpretations of Perrin’s work.},
  file = {files/220/Hudson - 2020 - The Reality of Jean Perrin's Atoms and Molecules.pdf}
}

