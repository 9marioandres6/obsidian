@article{orzackCriticalAssessmentLevinss1993,
  title = {A {{Critical Assessment}} of {{Levins}}'s {{The Strategy}} of {{Model Building}} in {{Population Biology}} (1966)},
  author = {Orzack, Steven and Sober, Elliott},
  date = {1993-12-01},
  journaltitle = {The Quarterly Review of Biology},
