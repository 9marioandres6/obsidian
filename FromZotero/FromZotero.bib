}

@article{padroApplicationRevisionIndirect2023,
  title = {Application and Revision of the Indirect Method for Determination of Asphaltene Precipitation Onsets for Light and Medium Oils in {{Argentina}}},
  author = {Padró, Juan M. and Novotny, Agostina and Smal, Clara and Cismondi, Martín},
  date = {2023-02-01},
  journaltitle = {Upstream Oil and Gas Technology},
